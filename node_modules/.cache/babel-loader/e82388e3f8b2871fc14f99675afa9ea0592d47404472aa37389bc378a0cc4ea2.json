{"ast":null,"code":"import { ref, onMounted, computed } from 'vue';\nimport { apiRequest } from '@/assets/js/apiRequest.js';\nimport ArgonButton from \"@/components/ArgonButton.vue\";\nconst perPage = 10; // Assuming 10 patients per page\n\nexport default {\n  __name: 'PatientsTable',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const patientsList = ref([]);\n    const currentPage = ref(1);\n    const getPatientsList = async () => {\n      try {\n        // Make API call to authenticate user\n        // Example using axios:\n        const response = await apiRequest('https://staging.itbrightsolution.com/ixora_backend/public/api/v1/patient/list');\n        patientsList.value = response.data;\n        console.log(patientsList.value);\n      } catch (error) {\n        console.error('Login failed:', error);\n        // Handle login error\n      }\n    };\n    onMounted(() => {\n      getPatientsList();\n    });\n\n    /// Function to calculate the total number of pages\n    const totalPages = computed(() => Math.ceil(patientsList.value.length / perPage));\n\n    // Function to get the subset of patients to display on the current page\n    const patientsToDisplay = computed(() => {\n      const startIndex = (currentPage.value - 1) * perPage;\n      const endIndex = startIndex + perPage;\n      return patientsList.value.slice(startIndex, endIndex);\n    });\n\n    // Function to navigate to a specific page\n    const goToPage = page => {\n      currentPage.value = page;\n    };\n\n    // Function to navigate to the next page\n    const nextPage = () => {\n      if (currentPage.value < totalPages.value) {\n        currentPage.value++;\n      }\n    };\n\n    // Function to navigate to the previous page\n    const prevPage = () => {\n      if (currentPage.value > 1) {\n        currentPage.value--;\n      }\n    };\n    const __returned__ = {\n      patientsList,\n      currentPage,\n      perPage,\n      getPatientsList,\n      totalPages,\n      patientsToDisplay,\n      goToPage,\n      nextPage,\n      prevPage,\n      ref,\n      onMounted,\n      computed,\n      get apiRequest() {\n        return apiRequest;\n      },\n      ArgonButton\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","apiRequest","ArgonButton","perPage","patientsList","currentPage","getPatientsList","response","value","data","console","log","error","totalPages","Math","ceil","length","patientsToDisplay","startIndex","endIndex","slice","goToPage","page","nextPage","prevPage"],"sources":["C:/Users/chink/Documents/GitHub/ixora-dashboard/src/views/components/PatientsTable.vue"],"sourcesContent":["<script setup>\r\nimport { ref, onMounted, computed } from 'vue';\r\nimport { apiRequest } from '@/assets/js/apiRequest.js'; \r\nimport ArgonButton from \"@/components/ArgonButton.vue\";\r\n\r\n\r\n  const patientsList = ref([]);\r\n  const currentPage = ref(1);\r\n  const perPage = 10; // Assuming 10 patients per page\r\n\r\n  const getPatientsList = async () => {\r\n    try {\r\n      // Make API call to authenticate user\r\n      // Example using axios:\r\n      const response = await apiRequest('https://staging.itbrightsolution.com/ixora_backend/public/api/v1/patient/list');\r\n      patientsList.value = response.data;\r\n      console.log(patientsList.value);\r\n    } catch (error) {\r\n      console.error('Login failed:', error);\r\n      // Handle login error\r\n    }\r\n  };\r\n\r\n  onMounted(() => {\r\n    getPatientsList();\r\n  });\r\n\r\n  /// Function to calculate the total number of pages\r\n  const totalPages = computed(() => Math.ceil(patientsList.value.length / perPage));\r\n\r\n  // Function to get the subset of patients to display on the current page\r\n  const patientsToDisplay = computed(() => {\r\n    const startIndex = (currentPage.value - 1) * perPage;\r\n    const endIndex = startIndex + perPage;\r\n    return patientsList.value.slice(startIndex, endIndex);\r\n  });\r\n\r\n  // Function to navigate to a specific page\r\n  const goToPage = (page) => {\r\n    currentPage.value = page;\r\n  };\r\n\r\n  // Function to navigate to the next page\r\n  const nextPage = () => {\r\n    if (currentPage.value < totalPages.value) {\r\n      currentPage.value++;\r\n    }\r\n  };\r\n\r\n  // Function to navigate to the previous page\r\n  const prevPage = () => {\r\n    if (currentPage.value > 1) {\r\n      currentPage.value--;\r\n    }\r\n  };\r\n\r\n</script>\r\n\r\n\r\n<template>\r\n  <div class=\"card\">\r\n    <div class=\"card-header pb-0\">\r\n      <h6>Patient List</h6>\r\n    </div>\r\n    <div class=\"card-body px-0 pt-0 pb-2\">\r\n      <div class=\"table-responsive p-0\">\r\n        <table v-if=\"patientsList\" class=\"table align-items-center mb-0\">\r\n          <thead>\r\n            <tr>\r\n              <th\r\n                class=\"text-uppercase text-secondary text-xxs font-weight-bolder opacity-7\"\r\n              >\r\n                Name\r\n              </th>\r\n              <th\r\n                class=\"text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7\"\r\n              >\r\n                Gender\r\n              </th>\r\n              <th\r\n                class=\"text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7\"\r\n              >\r\n                Created At\r\n              </th>\r\n              <th\r\n                class=\"text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7\"\r\n              >\r\n                Updated At\r\n              </th>\r\n              <th class=\"text-secondary opacity-7\"></th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(patient, index) in patientsToDisplay\" :key=\"index\">\r\n              <td>\r\n                <div class=\"d-flex px-2 py-1\">\r\n                  <div>\r\n                    <img\r\n                      :src=\"patient.profile_photo_path\"\r\n                      class=\"avatar avatar-sm me-3\"\r\n                      alt=\"user1\"\r\n                    />\r\n                  </div>\r\n                  <div class=\"d-flex flex-column justify-content-center\">\r\n                    <h6 class=\"mb-0 text-sm\">{{ patient.name }}</h6>\r\n                  </div>\r\n                </div>\r\n              </td>\r\n              <td class=\"align-middle text-center text-sm\">\r\n                <span class=\"badge badge-sm bg-gradient-success\">{{ patient.gender }}</span>\r\n              </td>\r\n              <td class=\"align-middle text-center\">\r\n                <span class=\"text-secondary text-xs font-weight-bold\"\r\n                  >{{ patient.created_at }}</span\r\n                >\r\n              </td>\r\n              <td class=\"align-middle text-center\">\r\n                <span class=\"text-secondary text-xs font-weight-bold\"\r\n                  >{{ patient.updated_at }}</span\r\n                >\r\n              </td>\r\n              <td class=\"align-middle\">\r\n                <a\r\n                  :href=\"`edit-patient/?uuid=${patient.uuid}`\"\r\n                  class=\"text-secondary font-weight-bold text-xs\"\r\n                  data-toggle=\"tooltip\"\r\n                  data-original-title=\"Edit user\"\r\n                  >\r\n                  <argon-button color=\"light\">\r\n                    <i class=\"fas fa-pen\"></i>\r\n                  </argon-button>\r\n                </a>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n        <div v-else class=\"text-center\">\r\n          <div class=\"fs-3 text-warning\">No any data</div>\r\n          <router-link to=\"/create-patient\">\r\n            <button class=\"btn btn-danger\" >ADD</button>\r\n          </router-link>\r\n          \r\n        </div>\r\n        <nav aria-label=\"Page navigation example\" class=\"my-3\">\r\n          <ul class=\"pagination justify-content-center\">\r\n            <li class=\"page-item\" :class=\"{ disabled: currentPage === 1 }\">\r\n              <a class=\"page-link\" tabindex=\"-1\" @click=\"prevPage\">\r\n                <i class=\"fa fa-angle-left\"></i>\r\n                <span class=\"sr-only\">Previous</span>\r\n              </a>\r\n            </li>\r\n            <li v-for=\"page in totalPages\" :key=\"page\" class=\"page-item\" :class=\"{ active: page === currentPage }\">\r\n              <a class=\"page-link\" @click=\"goToPage(page)\">{{ page }}</a>\r\n            </li>\r\n            <li class=\"page-item\" :class=\"{ disabled: currentPage === totalPages }\">\r\n              <a class=\"page-link\" @click=\"nextPage\">\r\n                <i class=\"fa fa-angle-right\"></i>\r\n                <span class=\"sr-only\">Next</span>\r\n              </a>\r\n            </li>\r\n          </ul>\r\n        </nav>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n"],"mappings":"AACA,SAASA,GAAG,EAAEC,SAAS,EAAEC,QAAQ,QAAQ,KAAK;AAC9C,SAASC,UAAU,QAAQ,2BAA2B;AACtD,OAAOC,WAAW,MAAM,8BAA8B;AAKpD,MAAMC,OAAO,GAAG,EAAE,CAAC,CAAC;;;;;;;;IAFpB,MAAMC,YAAY,GAAGN,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMO,WAAW,GAAGP,GAAG,CAAC,CAAC,CAAC;IAG1B,MAAMQ,eAAe,GAAG,MAAAA,CAAA,KAAY;MAClC,IAAI;QACF;QACA;QACA,MAAMC,QAAQ,GAAG,MAAMN,UAAU,CAAC,+EAA+E,CAAC;QAClHG,YAAY,CAACI,KAAK,GAAGD,QAAQ,CAACE,IAAI;QAClCC,OAAO,CAACC,GAAG,CAACP,YAAY,CAACI,KAAK,CAAC;MACjC,CAAC,CAAC,OAAOI,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,eAAe,EAAEA,KAAK,CAAC;QACrC;MACF;IACF,CAAC;IAEDb,SAAS,CAAC,MAAM;MACdO,eAAe,CAAC,CAAC;IACnB,CAAC,CAAC;;IAEF;IACA,MAAMO,UAAU,GAAGb,QAAQ,CAAC,MAAMc,IAAI,CAACC,IAAI,CAACX,YAAY,CAACI,KAAK,CAACQ,MAAM,GAAGb,OAAO,CAAC,CAAC;;IAEjF;IACA,MAAMc,iBAAiB,GAAGjB,QAAQ,CAAC,MAAM;MACvC,MAAMkB,UAAU,GAAG,CAACb,WAAW,CAACG,KAAK,GAAG,CAAC,IAAIL,OAAO;MACpD,MAAMgB,QAAQ,GAAGD,UAAU,GAAGf,OAAO;MACrC,OAAOC,YAAY,